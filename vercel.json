{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_GRAPHQL_API": "@next_public_graphql_api",
    "AUTH0_DOMAIN": "@auth0_domain",
    "AUTH0_CLIENT_ID": "@auth0_client_id"
  },
  "build": {
    "env": {
      "NEXT_AUTH_SECRET": "@next_auth_secret",
      "AUTH0_CLIENT_SECRET": "@auth0_client_secret"
    }
  }
}
